/**index.wxss**/

.container {
  margin: 0em;
  padding: 0em;
  background: #fff;
}

#searchBar {
  display: block;
}

#pickView {
  display: flex;
}

.pick {
  padding: 0.3em;
  display: flex;
}

#listView {
